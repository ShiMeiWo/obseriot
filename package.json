{
  "name": "obseriot",
  "version": "0.3.7",
  "main": "dist/obseriot.js",
  "jsnext:main": "src/obseriot.js",
  "scripts": {
    "test": "karma start config/karma.conf.js",
    "build": "xo && node config/rollup.build.js"
  },
  "author": "aggre",
  "license": "MIT",
  "dependencies": {
    "riot-observable": "3.0.0"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "mocha": "^3.2.0",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "xo": "^0.18.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obseriot/obseriot.git"
  },
  "bugs": {
    "url": "https://github.com/obseriot/obseriot/issues"
  },
  "homepage": "https://github.com/obseriot/obseriot",
  "description": "Simple observer pattern",
  "xo": {
    "envs": [
      "browser",
      "node",
      "es6",
      "mocha"
    ],
    "rules": {
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "computed-property-spacing": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        "always"
      ],
      "space-in-parens": [
        2,
        "always"
      ],
      "space-before-blocks": 2
    },
    "overrides": [
      {
        "files": "test/*.js",
        "rules": {
          "block-scoped-var": 0
        }
      }
    ],
    "semicolon": false
  }
}
